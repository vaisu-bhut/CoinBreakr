{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_DOMAIN_URL": "$API_DOMAIN_URL",
        "EXPO_PUBLIC_API_FALLBACK_IP": "$API_FALLBACK_IP",
        "EXPO_PUBLIC_API_PORT": "$API_PORT",
        "EXPO_PUBLIC_API_VERSION": "$EXPO_PUBLIC_API_VERSION",
        "EXPO_PUBLIC_API_TIMEOUT": "$EXPO_PUBLIC_API_TIMEOUT",
        "EXPO_PUBLIC_API_RETRY_ATTEMPTS": "$EXPO_PUBLIC_API_RETRY_ATTEMPTS",
        "EXPO_PUBLIC_API_RETRY_DELAY": "$EXPO_PUBLIC_HEALTH_CHECK_TIMEOUT"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_DOMAIN_URL": "$API_DOMAIN_URL",
        "EXPO_PUBLIC_API_FALLBACK_IP": "$API_FALLBACK_IP",
        "EXPO_PUBLIC_API_PORT": "$API_PORT",
        "EXPO_PUBLIC_API_VERSION": "$EXPO_PUBLIC_API_VERSION",
        "EXPO_PUBLIC_API_TIMEOUT": "$EXPO_PUBLIC_API_TIMEOUT",
        "EXPO_PUBLIC_API_RETRY_ATTEMPTS": "$EXPO_PUBLIC_API_RETRY_ATTEMPTS",
        "EXPO_PUBLIC_API_RETRY_DELAY": "$EXPO_PUBLIC_HEALTH_CHECK_TIMEOUT"
      }
    }
  },
  "submit": {
    "production": {}
  }
}