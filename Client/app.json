{
  "expo": {
    "name": "CoinBreakr",
    "slug": "CoinBreakr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#F8FAFC",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8FAFC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coinbreaker.app",
      "versionCode": 1,
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to contacts to help you find friends who are already using CoinBreakr and make it easier to split expenses."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8FAFC"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.coinbreaker.app",
      "predictiveBackGestureEnabled": false,
      "versionCode": 1,
      "permissions": [
        "android.permission.READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}